      "cell_type": "code",
      "source": [
        "# ✅ Установка библиотек и клонирование репозиториев\n",
        "!git clone https://github.com/ultralytics/yolov5\n",
        "!git clone https://github.com/AndreyLiskovets/yolov5-hic.git\n",
        "!pip install -r yolov5/requirements.txt\n",
        "!pip install -r yolov5-hic/requirements.txt\n",
        "!pip install roboflow matplotlib\n",
        "\n",
        "# ✅ Загрузка датасета (детекция людей, ~500 изображений)\n",
        "from roboflow import Roboflow\n",
        "rf = Roboflow(api_key=\"rfU8nUXhxDZ0T6a3iwLV\")  # ← публичный API ключ для demo датасета\n",
        "project = rf.workspace(\"roboflow-universe-projects\").project(\"person-detection-bp7pe\")\n",
        "dataset = project.version(1).download(\"yolov5\")\n",
        "\n",
        "# ✅ Конфигурация датасета\n",
        "import yaml\n",
        "\n",
        "yaml_path = f\"{dataset.location}/data.yaml\"\n",
        "with open(yaml_path, 'r') as f:\n",
        "    data_yaml = yaml.safe_load(f)\n",
        "\n",
        "print(\"Классы:\", data_yaml['names'])\n",
        "\n",
        "# ✅ Обучение YOLOv5\n",
        "!python yolov5/train.py --img 640 --batch 16 --epochs 20 \\\n",
        "    --data {yaml_path} --weights '' --cfg yolov5/models/yolov5s.yaml --name yolov5_person\n",
        "\n",
        "# ✅ Обучение YOLOv5-HIC\n",
        "!python yolov5-hic/train.py --img 640 --batch 16 --epochs 20 \\\n",
        "    --data {yaml_path} --weights '' --cfg yolov5-hic/models/yolov5s_hic.yaml --name yolov5hic_person\n",
        "\n",
        "# ✅ Оценка точности\n",
        "!python yolov5/val.py --weights yolov5/runs/train/yolov5_person/weights/best.pt --data {yaml_path} --img 640 --task val --name yolov5_val\n",
        "!python yolov5-hic/val.py --weights yolov5-hic/runs/train/yolov5hic_person/weights/best.pt --data {yaml_path} --img 640 --task val --name yolov5hic_val\n",
        "\n",
        "# ✅ Измерение времени инференса\n",
        "import torch\n",
        "import time\n",
        "from pathlib import Path\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "sample_image = list(Path(f\"{dataset.location}/valid/images\").glob(\"*.jpg\"))[0]\n",
        "\n",
        "model_v5 = torch.hub.load('yolov5', 'custom', path='yolov5/runs/train/yolov5_person/weights/best.pt', source='local')\n",
        "start = time.time()\n",
        "results_v5 = model_v5(sample_image)\n",
        "inference_v5 = time.time() - start\n",
        "\n",
        "model_hic = torch.hub.load('yolov5-hic', 'custom', path='yolov5-hic/runs/train/yolov5hic_person/weights/best.pt', source='local')\n",
        "start = time.time()\n",
        "results_hic = model_hic(sample_image)\n",
        "inference_hic = time.time() - start\n",
        "\n",
        "# ✅ Извлечение mAP\n",
        "def get_map(result_txt_path):\n",
        "    with open(result_txt_path, 'r') as f:\n",
        "        for line in f.readlines():\n",
        "            if 'mAP@.5' in line:\n",
        "                return float(line.strip().split()[-1])\n",
        "    return 0.0\n",
        "\n",
        "map_v5 = get_map(\"yolov5/runs/val/yolov5_val/results.txt\")\n",
        "map_hic = get_map(\"yolov5-hic/runs/val/yolov5hic_val/results.txt\")\n",
        "\n",
        "# ✅ Построение графика сравнения\n",
        "models = ['YOLOv5', 'YOLOv5-HIC']\n",
        "maps = [map_v5, map_hic]\n",
        "times = [inference_v5, inference_hic]\n",
        "\n",
        "plt.figure(figsize=(12, 5))\n",
        "\n",
        "plt.subplot(1, 2, 1)\n",
        "plt.bar(models, maps, color=['skyblue', 'salmon'])\n",
        "plt.title('mAP@0.5 Comparison')\n",
        "plt.ylabel('mAP')\n",
        "\n",
        "plt.subplot(1, 2, 2)\n",
        "plt.bar(models, times, color=['skyblue', 'salmon'])\n",
        "plt.title('Inference Time Comparison')\n",
        "plt.ylabel('Seconds')\n",
        "\n",
        "plt.tight_layout()\n",
        "plt.show()\n",
        "\n",
        "# ✅ Вывод результатов\n",
        "print(f\"YOLOv5:     mAP@0.5 = {map_v5:.3f}, Inference Time = {inference_v5:.3f}s\")\n",
        "print(f\"YOLOv5-HIC: mAP@0.5 = {map_hic:.3f}, Inference Time = {inference_hic:.3f}s\")\n"
      ],
